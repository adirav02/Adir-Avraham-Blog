---
import { db } from "astro:db";
import BaseLayout from "../../layouts/BaseLayout.astro";
import { Newsletter } from "astro:db";

const emails = await db.select().from(Newsletter);
export const prerender = false;
---

<BaseLayout>

    <div class="flex flex-col gap-3 text-3xl">
        {emails.map(email => <p>fullName: {email.fullName} email: {email.email}</p>)}
    </div>

</BaseLayout>