---
const {title, description, img, alt} = Astro.props;
---



<div class="article">
    <a href="#">
        <img
            src={img}
            class="article-img"
            alt={alt}
        />
    </a>

    <div class="article-content">
        <p class="article-title">
            <a href="#"> {title}</a>
        </p>
        <p class="article-description">{description}</p>
    </div>
</div>

